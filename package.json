{
  "name": "moment-jalaali-es",
  "version": "1.0.0",
  "description": "Jalaali (Jalali, Persian, Khorshidi, Shamsi) calendar system plugin for moment.js.",
  "license": "MIT",
  "homepage": "https://github.com/persiandate/moment-jalaali-es",
  "contributors": [
    "Behrang Noruzi Niya <behrangn@gmail.com>",
    "Pooya Parsa <pooya@pi0.ir>"
  ],
  "main": "dist/moment-jalaali.common.js",
  "module": "dist/moment-jalaali.es.js",
  "browser": "dist/moment-jalaali.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha --require source-map-support/register test",
    "lint": "eslint src",
    "build": "bili -n moment-jalaali",
    "prepare": "npm run build && npm run test",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "files": [
    "dist",
    "src"
  ],
  "bili": {
    "entry": "src/index.js",
    "format": [
      "cjs",
      "umd",
      "es"
    ],
    "map": true,
    "compress": "umd",
    "external": [
      "moment",
      "jalaali-js"
    ]
  },
  "dependencies": {
    "jalaali-js": "^1.0.0",
    "moment": "^2.19.0"
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "chai": "^4.0.2",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2",
    "source-map-support": "^0.5.0",
    "standard-version": "^4.2.0"
  },
  "keywords": [
    "jalaali",
    "jalali",
    "persian",
    "khorshidi",
    "shamsi",
    "date",
    "calendar",
    "conversion",
    "convert",
    "es",
    "esm",
    "module"
  ]
}
